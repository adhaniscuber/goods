{
  "name": "@pomona/goods-core",
  "version": "1.0.24",
  "description": "Pomona Goods Design System - Core and Basics",
  "author": "Pomona IT",
  "homepage": "https://github.com/PT-Tennova-Cipta-Inatech/pomona3-ui/tree/goods_master/packages/goods-core",
  "license": "ISC",
  "main": "./lib/index.js",
  "module": "./lib/packages/goods-core/src/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf tsc-out",
    "compile": "yarn clean && yarn rollup",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "yarn compile",
