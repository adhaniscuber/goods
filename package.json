  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run clean",
    "lerna": "lerna",
    "lint": "lerna run lint",
    "report-coverage": "yarn workspace @pomona/goods-core run report-coverage",
    "storybook": "start-storybook -p 9001 -c .storybook-react -s .storybook-react/public",
    "storybook:build": "rimraf dist-web && build-storybook -c .storybook-react -o dist-web && node ./.storybook-react/after-build",
    "storybook:sync": "aws s3 sync dist-web/ s3://pomona-design-system/ --profile goods --cache-control no-cache",
    "storybook:deploy": "yarn run storybook:build && yarn run storybook:sync",
    "test": "yarn workspace @pomona/goods-core run test",
    "test:coverage": "yarn workspace @pomona/goods-core run test:coverage",
    "validate": "lerna run validate"
  },
